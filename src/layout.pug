doctype html
block beforehtml
html
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover, user-scalable=no")
    title= title
    script(async, src="https://www.googletagmanager.com/gtag/js?id=UA-39556213-3")
    script.
      window.dataLayer = window.dataLayer || []
      function gtag () { dataLayer.push(arguments) } // eslint-disable-line
      gtag('js', new Date())
      gtag('config', 'UA-39556213-3')
    if watch
      script.
        const hostname = (location.host || 'localhost').split(':')[0]
        document.write(`<script src="http://${hostname}:35729/livereload.js?snipver=1"></' + 'script>`)
    link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/bootstrap@4/dist/css/bootstrap.min.css")
    link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/font-awesome@4/css/font-awesome.min.css")
    link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/sweetalert2@9/dist/sweetalert2.min.css")
    link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/@sweetalert2/theme-default@3/default.min.css")
    block style
  body
    block content
    script(src="https://cdn.jsdelivr.net/npm/jquery@3/dist/jquery.min.js")
    script(src="https://cdn.jsdelivr.net/npm/popper.js@1/dist/umd/popper.min.js")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@4/dist/js/bootstrap.min.js")
    script(src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.min.js")
    script(src="https://cdn.jsdelivr.net/npm/axios@0.19.2/dist/axios.min.js")
    script(src="https://cdn.jsdelivr.net/npm/qs@6/dist/qs.min.js")
    script(src="https://cdn.jsdelivr.net/npm/lodash@4/lodash.min.js")
    script(src="https://cdn.jsdelivr.net/npm/sweetalert2@9")
    block script
